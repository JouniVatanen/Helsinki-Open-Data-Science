x2 <- rnorm(100)
y <- 2*x1 + -1*x2 + rnorm(100, sd = 1)
mycol <- rgb(t(col2rgb("blue")), alpha = 150, maxColorValue = 255)
p <- scatterplot3d(x = x1, y = x2, z = y, grid = T, box = F, mar = c(3,3,3,3), tick.marks = F, pch = 16, color = mycol)
p$plane3d(lm(y~x1+x2))
